{
  "title": "Volume Zones",
  "type": "array",
  "uniqueItemProperties": [
    "name"
  ],
  "items": {
    "title": "Model Type",
    "type": "object",
    "properties": {
      "name": {
        "title": "Name",
        "type": "string",
        "readOnly": true
      },
      "modelType": {
        "title": "Type",
        "enum": [
          "FluidDynamics",
          "HeatTransfer"
        ],
        "default": "FluidDynamics"
      }
    },
    "dependencies": {
      "modelType": {
        "oneOf": [
          {
            "title": "FluidDynamics",
            "type": "object",
            "properties": {
              "modelType": {
                "const": "FluidDynamics"
              },
              "_hasReferenceFrame": {
                "title": "Has reference frame?",
                "type": "boolean",
                "default": false
              }
            },
            "dependencies": {
              "_hasReferenceFrame": {
                "oneOf": [
                  {
                    "properties": {
                      "_hasReferenceFrame": {
                        "const": true
                      },
                      "referenceFrame": {
                        "title": "Reference Frame",
                        "properties": {
                          "centerOfRotation": {
                            "title": "Center of Rotation",
                            "$ref": "#/definitions/vector3"
                          },
                          "axisOfRotation": {
                            "title": "Axis of Rotation",
                            "$ref": "#/definitions/vector3"
                          },
                          "parentVolumeName": {
                            "title": "Parent Volume Name",
                            "type": "string"
                          },
                          "_type": {
                            "title": "Rotational Speed",
                            "enum": [
                              "RPM",
                              "omegaRadians",
                              "omegaDegrees",
                              "thetaRadians",
                              "thetaDegrees"
                            ],
                            "default": "RPM"
                          }
                        },
                        "required": [
                          "centerOfRotation",
                          "axisOfRotation"
                        ],
                        "dependencies": {
                          "_type": {
                            "oneOf": [
                              {
                                "title": "RPM",
                                "type": "object",
                                "properties": {
                                  "_type": {
                                    "const": "RPM"
                                  },
                                  "_RPM": {
                                    "title": "RPM",
                                    "type": "number"
                                  },
                                  "omegaRadians": {
                                    "title": "Omega",
                                    "unit": "rad",
                                    "type": "number",
                                    "readOnly": true
                                  }
                                },
                                "required": [
                                  "_RPM"
                                ]
                              },
                              {
                                "title": "Omega Radians",
                                "type": "object",
                                "properties": {
                                  "_type": {
                                    "const": "omegaRadians"
                                  },
                                  "omegaRadians": {
                                    "title": "Omega",
                                    "unit": "rad",
                                    "type": "number"
                                  }
                                },
                                "required": [
                                  "omegaRadians"
                                ]
                              },
                              {
                                "title": "Omega Degrees",
                                "type": "object",
                                "properties": {
                                  "_type": {
                                    "const": "omegaDegrees"
                                  },
                                  "omegaDegrees": {
                                    "title": "Omega",
                                    "unit": "deg",
                                    "type": "number"
                                  }
                                },
                                "required": [
                                  "omegaDegrees"
                                ]
                              },
                              {
                                "title": "Theta Radians",
                                "type": "object",
                                "properties": {
                                  "_type": {
                                    "const": "thetaRadians"
                                  },
                                  "thetaRadians": {
                                    "title": "Theta",
                                    "unit": "rad",
                                    "type": "string"
                                  }
                                },
                                "required": [
                                  "thetaRadians"
                                ]
                              },
                              {
                                "title": "Theta Degrees",
                                "type": "object",
                                "properties": {
                                  "_type": {
                                    "const": "thetaDegrees"
                                  },
                                  "thetaDegrees": {
                                    "title": "Theta",
                                    "unit": "deg",
                                    "type": "string"
                                  }
                                },
                                "required": [
                                  "thetaDegrees"
                                ]
                              }
                            ]
                          }
                        }
                      }
                    },
                    "required": [
                      "referenceFrame"
                    ]
                  },
                  {
                    "properties": {
                      "_hasReferenceFrame": {
                        "const": false
                      }
                    }
                  }
                ]
              }
            }
          },
          {
            "title": "HeatTransfer",
            "type": "object",
            "properties": {
              "modelType": {
                "const": "HeatTransfer"
              },
              "thermalConductivity": {
                "title": "Thermal Conductivity",
                "exclusiveMinimum": 0,
                "type": "number"
              },
              "heatCapacity": {
                "title": "Heat Capacity",
                "type": "number",
                "exclusiveMinimum": 0
              },
              "volumetricHeatSource": {
                "title": "Volumetric Heat Source",
                "oneOf": [
                  {
                    "title": "Value",
                    "type": "number",
                    "minimum": 0
                  },
                  {
                    "title": "Expression",
                    "type": "string"
                  }
                ]
              },
              "initialCondition": {
                "title": "Initial Condition Heat Transfer",
                "type": "object",
                "properties": {
                  "T_solid": {
                    "title": "Initial Condition - Temperature Solid",
                    "anyOf": [
                      {
                        "title": "Value",
                        "type": "number",
                        "exclusiveMinimum": 0
                      },
                      {
                        "title": "Expression",
                        "type": "string"
                      }
                    ]
                  }
                }
              }
            },
            "required": [
              "thermalConductivity"
            ]
          }
        ]
      }
    }
  },
  "definitions": {
    "vector3": {
      "type": "array",
      "items": {
        "type": "number"
      },
      "strictType": {
        "type": "vector3"
      }
    }
  }
}